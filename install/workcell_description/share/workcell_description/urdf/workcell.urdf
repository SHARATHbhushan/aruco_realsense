  <!-- Specify some standard inertial calculations https://en.wikipedia.org/wiki/List_of_moments_of_inertia -->
  <!-- These make use of xacro's mathematical functionality -->
  <material name="blue">
    <color rgba="0 0 1 1"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <!--Base plate-->
  <link name="grippy_base_link">
    <visual>
      <geometry>
        <cylinder length="0.015" radius="0.0375"/>
      </geometry>
      <material name="blue"/>
      <origin rpy="0 0 0" xyz="0 0 0.0075"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.015" radius="0.0375"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.0075"/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.0075"/>
      <mass value="0.2"/>
      <inertia ixx="7.406249999999999e-05" ixy="0.0" ixz="0.0" iyy="7.406249999999999e-05" iyz="0.0" izz="0.000140625"/>
    </inertial>
  </link>
  <!--Main gripper-->
  <link name="grippy_body_link">
    <visual>
      <geometry>
        <box size="0.05 0.03 0.1044"/>
      </geometry>
      <material name="green"/>
      <origin rpy="0 0 0 " xyz="0 0 0.0522"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.03 0.1044"/>
      </geometry>
      <origin rpy="0 0 0 " xyz="0 0 0.0522"/>
    </collision>
    <inertial>
      <mass value="0.51"/>
      <inertia ixx="0.0007" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0006"/>
    </inertial>
    <inertial>
      <origin rpy="0 0 0 " xyz="0 0 0.0522"/>
      <mass value="0.5"/>
      <inertia ixx="0.00049164" ixy="0.0" ixz="0.0" iyy="0.0005583066666666667" iyz="0.0" izz="0.00014166666666666668"/>
    </inertial>
  </link>
  <!--Pincer block 1-->
  <link name="grippy_left_pincer_link">
    <visual>
      <geometry>
        <box size="0.008 0.018 0.014"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.008 0.018 0.014"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="4.333333333333333e-06" ixy="0.0" ixz="0.0" iyy="2.166666666666667e-06" iyz="0.0" izz="3.233333333333333e-06"/>
    </inertial>
  </link>
  <!--Pincer block 2-->
  <link name="grippy_right_pincer_link">
    <visual>
      <geometry>
        <box size="0.008 0.018 0.014"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.008 0.018 0.014"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="4.333333333333333e-06" ixy="0.0" ixz="0.0" iyy="2.166666666666667e-06" iyz="0.0" izz="3.233333333333333e-06"/>
    </inertial>
  </link>
  <!-- Virtual link for visualizytion and planning-->
  <link name="tool_tip"/>
  <!-- Joints-->
  <joint name="grippy_body_joint" type="fixed">
    <parent link="grippy_base_link"/>
    <child link="grippy_body_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.015"/>
  </joint>
  <joint name="grippy_pincer_left_joint" type="prismatic">
    <parent link="grippy_body_link"/>
    <child link="grippy_left_pincer_link"/>
    <origin rpy="0 0 0" xyz="-0.021 0 0.11140000000000001"/>
    <!-- the motion of the pincer is along the X axis -->
    <axis xyz="1 0 0"/>
    <!-- the motion of the pincer is limited to the half of the gripper body length -->
    <limit effort="100" lower="0" upper="0.0125" velocity="100"/>
  </joint>
  <joint name="grippy_pincer_right_joint" type="prismatic">
    <parent link="grippy_body_link"/>
    <child link="grippy_right_pincer_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.021 0 0.11140000000000001"/>
    <!-- the motion of the pincer is along the X axis -->
    <axis xyz="-1 0 0"/>
    <!-- the motion of the pincer is limited to the half of the gripper body length -->
    <limit effort="100" lower="0" upper="0.0125" velocity="100"/>
  </joint>
  <joint name="grippy_tip_joint" type="fixed">
    <parent link="grippy_body_link"/>
    <child link="tool_tip"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.1044"/>
  </joint>
  <material name="aluminum">
    <color rgba="0.5 0.5 0.5 1"/>
  </material>
  <material name="plastic">
    <color rgba="0.1 0.1 0.1 1"/>
  </material>
  <!-- camera body, with origin at bottom screw mount -->
  <joint name="camera_joint" type="fixed">
    <origin rpy="1.5707963267948966 1.5707963267948966 -3.141592653589793" xyz="0.5 -0.5 1"/>
    <parent link="robotcell_link"/>
    <child link="camera_bottom_screw_frame"/>
  </joint>
  <link name="camera_bottom_screw_frame"/>
  <joint name="camera_link_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.010600000000000002 0.0175 0.0125"/>
    <parent link="camera_bottom_screw_frame"/>
    <child link="camera_link"/>
  </joint>
  <link name="camera_link">
    <visual>
      <!-- the mesh origin is at front plate in between the two infrared camera axes -->
      <origin rpy="1.5707963267948966 0 1.5707963267948966" xyz="0.0043 -0.0175 0"/>
      <geometry>
        <mesh filename="package://realsense2_description/meshes/d435.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -0.0175 0"/>
      <geometry>
        <box size="0.02505 0.09 0.025"/>
      </geometry>
    </collision>
    <inertial>
      <!-- The following are not reliable values, and should not be used for modeling -->
      <mass value="0.072"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.003881243" ixy="0.0" ixz="0.0" iyy="0.000498940" iyz="0.0" izz="0.003879257"/>
    </inertial>
  </link>
