  </joint>
  <!-- shunk_egp50 or grippy -->
  <joint name="robot_to_gripper_joint" type="fixed">
    <parent link="wrist_3_link"/>
    <child link="grippy_base_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
  </joint>
  <!-- Uncomment 2 lines below and comment grippy to use shunk_egp50 Gripper  -->
